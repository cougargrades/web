{
  "name": "cougar-grades",
  "version": "0.0.0-development",
  "description": "Web application",
  "private": true,
  "dependencies": {
    "better-sqlite3": "^5.4.0",
    "chalk": "^2.4.2",
    "convert-hrtime": "^3.0.0",
    "dotenv": "^8.0.0",
    "fastify": "^2.3.0",
    "fastify-cli": "^1.0.0",
    "fastify-graceful-shutdown": "^2.0.1",
    "fastify-rate-limit": "^2.1.1",
    "fastify-response-time": "github:lolo32/fastify-response-time",
    "fastify-static": "^2.4.0",
    "make-promises-safe": "^5.0.0",
    "mysql2": "^1.6.5",
    "node-fetch": "^2.5.0",
    "pretty-hrtime": "^1.0.3",
    "prop-types": "^15.7.2",
    "react-google-charts": "^3.0.13",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "babel-loader": "^8.0.6",
    "handlebars": "^4.1.2",
    "nodemon": "^1.19.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "sass": "^1.20.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "config": {
    "server": {
      "port": 3000
    }
  },
  "scripts": {
    "start": "npm run prod",
    "prod": "npm run prod:build && npm run prod:server",
    "prod:server": "node server.js",
    "prod:build": "npm run build",
    "build": "npm run build:preprocess && npm run build:mustache && npm run build:webpack && npm run build:sass",
    "build:preprocess": "./front/preprocess.sh",
    "build:mustache": "./front/generate_html.js",
    "build:webpack": "npx webpack --mode production",
    "build:sass": "npx sass ./front/sass:./front/_site/css/",
    "dev": "npm run dev:mustache & npm run dev:webpack & npm run dev:sass & npm run dev:nodemon & npm run dev:static",
    "dev:nodemon": "npx nodemon --ignore front/ --exec node --inspect=0.0.0.0:9229 server.js",
    "dev:mustache": "npx nodemon --watch front/html/ --ext hbs --exec ./front/generate_html.js",
    "dev:webpack": "npx nodemon --watch front/js/ --ext js,jsx --exec npx webpack --mode development",
    "dev:sass": "npx sass --watch ./front/sass:./front/_site/css/ -I ./front/sass/partials/",
    "dev:static": "npx nodemon --watch front/static/ --ext css --exec cp -r ./front/static/* ./front/_site/",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/au5ton/cougar-grades.git"
  },
  "license": "ISC",
  "publishConfig": {
    "access": "restricted"
  }
}
