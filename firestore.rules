rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /catalog/{course} {
      allow read: if true;
      allow write: if false;
    }
    match /catalog_meta/{meta} {
    	allow read: if true;
      allow write: if false;
    }
    match /instructors/{name} {
    	allow read: if true;
      allow write: if false;
    }
  }
}