{
  "name": "@cougargrades/web",
  "version": "1.0.0",
  "scripts": {
    "start": "SNOWPACK_PUBLIC_GIT_SHA=$(git rev-parse --short HEAD) SNOWPACK_PUBLIC_VERSION=$npm_package_version SNOWPACK_PUBLIC_BUILD_DATE=$(date) snowpack dev",
    "build": "SNOWPACK_PUBLIC_GIT_SHA=$(git rev-parse --short HEAD) SNOWPACK_PUBLIC_VERSION=$npm_package_version SNOWPACK_PUBLIC_BUILD_DATE=$(date) snowpack build",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "license": "MIT",
  "dependencies": {
    "@cougargrades/types": "0.0.50",
    "@exampledev/new.css": "^1.1.3",
    "firebase": "^8.2.10",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-dropzone": "^11.3.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactfire": "^3.0.0-rc.0",
    "swr": "^0.4.2",
    "timeago-react": "^3.0.0"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.4.0",
    "@snowpack/plugin-sass": "^1.1.1",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router": "^5.1.12",
    "@types/react-router-dom": "^5.1.7",
    "@types/snowpack-env": "^2.3.3",
    "prettier": "^2.0.5",
    "snowpack": "^3.0.13",
    "typescript": "^4.2.2"
  }
}
