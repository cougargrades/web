{
  "name": "@cougargrades/web",
  "version": "1.0.0",
  "scripts": {
    "start": "REACT_APP_GIT_SHA=$(git rev-parse --short HEAD) REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_DATE=$(date) react-scripts start",
