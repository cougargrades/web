  "engines": {
    "node": ">=16 <=24"
  },
  "scripts": {
    "dev": "NEXT_PUBLIC_GIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_VERSION=$npm_package_version NEXT_PUBLIC_BUILD_DATE=$(date) next dev",
    "dev:win32": "cross-env NODE_OPTIONS=\"--inspect\" NEXT_PUBLIC_GIT_SHA=$(git rev-parse --short HEAD) NEXT_PUBLIC_VERSION=$npm_package_version NEXT_PUBLIC_BUILD_DATE=$(date) next dev",
    "build": "NEXT_PUBLIC_VERCEL_ENV=$VERCEL_ENV NEXT_PUBLIC_GIT_SHA=$VERCEL_GIT_COMMIT_SHA NEXT_PUBLIC_VERSION=$npm_package_version NEXT_PUBLIC_BUILD_DATE=$(date) next build",
    "build:win32": "cross-env NEXT_PUBLIC_VERCEL_ENV=$VERCEL_ENV NEXT_PUBLIC_GIT_SHA=$VERCEL_GIT_COMMIT_SHA NEXT_PUBLIC_VERSION=$npm_package_version NEXT_PUBLIC_BUILD_DATE=$(date) next build",
    "start": "next start",
    "lint": "next lint",
    "betterlint": "tsc --noEmit",
    "preview": "opennextjs-cloudflare build && wrangler dev",
    "preview:win32": "opennextjs-cloudflare build; wrangler dev",
    "deploy": "opennextjs-cloudflare build && wrangler deploy",
    "deploy:win32": "opennextjs-cloudflare build; wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "overrides": {
    
  },
