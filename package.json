{
  "name": "@cougargrades/web",
  "version": "1.0.0",
  "scripts": {
    "start": "SNOWPACK_PUBLIC_GIT_SHA=$(git rev-parse --short HEAD) SNOWPACK_PUBLIC_VERSION=$npm_package_version SNOWPACK_PUBLIC_BUILD_DATE=$(date) snowpack dev",
    "build": "SNOWPACK_PUBLIC_GIT_SHA=$(git rev-parse --short HEAD) SNOWPACK_PUBLIC_VERSION=$npm_package_version SNOWPACK_PUBLIC_BUILD_DATE=$(date) snowpack build",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lint": "prettier --check 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "license": "MIT",
  "dependencies": {
    "@exampledev/new.css": "^1.1.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "swr": "^0.3.0",
    "timeago-react": "^3.0.0"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.0.4",
    "@snowpack/plugin-optimize": "^0.2.9",
    "@snowpack/plugin-react-refresh": "^2.3.7",
    "@snowpack/plugin-sass": "^1.1.1",
    "@snowpack/plugin-typescript": "^1.1.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/snowpack-env": "^2.3.2",
    "prettier": "^2.0.5",
    "snowpack": "^2.17.1",
    "typescript": "^4.1.0"
  }
}
